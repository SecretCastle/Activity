.container {
  background: #eee;
}

.member {
  padding: 10px;
  border-bottom: 1px solid #ccc;
  background: #fff;
}

.avator {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid #ccc;
}

.avator image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.username {
  width: 90%;
  padding-left: 10px;
  line-height: 52px;
  font-size: 36rpx;
}

.hasOrder {
  margin-top: 10px;
}

.hasOrder .wrap, .hasAttendAct .wrap {
  background: #fff;
}

.hasAttendAct {
  margin-top: 10px;
}

.title_tip {
  padding-left: 10px;
  line-height: 28px;
  color: #888;
}
